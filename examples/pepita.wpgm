import pepita.*

program pepitaFuerte {
	console.println("En la situación inicial")
	console.println("¿Pepita es fuerte?")
	console.println(pepita.esFuerte())
	pepita.volar(60)
	console.println("Luego de volar 60 kilómetros")
	console.println("¿Pepita es fuerte?")
	console.println(pepita.esFuerte())
}
